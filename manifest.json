{
  "manifest_version": 3,
  "name": "MyPlan Professor Rating Extension",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://www.ratemyprofessors.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://myplan.uw.edu/*"],
      "js": ["content.js"]
    }
  ]
}